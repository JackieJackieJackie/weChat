/*弹窗  */

.shade_box {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.shade_bg {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  left: 0;
  animation: shadeBg 0.3s 0.1s ease forwards;
  opacity: 0;
}

.garden-modal-order {
  border-radius: 10rpx;
  padding: 80rpx 50rpx;
  animation: show 0.8s 0.4s ease forwards;
  z-index: 1;
  opacity: 0;
}
.garden-modal-strategy {
  border-radius: 10rpx;
  animation: show 0.8s 0.4s ease forwards;
  z-index: 1;
  opacity: 0;
  display: flex;
  flex-direction: column;
}
@keyframes shadeBg {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes show {
  0% {
    transform: scale(0);
    opacity: 0;
  }

  33% {
    transform: scale(1.1);
    opacity: 1;
  }

  55% {
    transform: scale(0.95);
    opacity: 1;
  }

  100% {
    transform: scale(1);
    opacity: 1;
  }
}
