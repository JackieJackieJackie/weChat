/* pages/sponsor/settings/index.wxss */
.settings-view {
  width: 100%;
  height: 100%;
  position: fixed;  
  display: flex;
  background: #ffdac7;
  flex-direction: column;
  font-size: 30rpx;
}
.settings-view-toolbar{
    width: 100%;
  height: 132rpx;
  justify-content: center;
  display: flex;
  background: #fd957f;
}

.settings-contact {
   font-size: 30rpx;
  position: absolute;
}
.settings-view-myService{
   background-color: #ffede3;
  height: 120rpx;
  border-radius: 34rpx 34rpx 34rpx 34rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 70rpx;
  display: flex;
  align-items: center;
   padding: 0 34rpx 0 34rpx;
}
.settings-view-myService2{
   background-color: #ffede3;
  height: 120rpx;
  border-radius: 34rpx 34rpx 34rpx 34rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
   padding: 0 34rpx 0 34rpx;
}
.cs_button {
  background-color: #ffede3;
  height: 120rpx;
  border-radius: 34rpx 34rpx 34rpx 34rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
   padding: 0 34rpx 0 34rpx;
}

.cs_button:after {
  border: 0px;
 
}
.navigator-arrow {
   position: absolute;
   right: 40rpx;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}
.navigator-arrow2 {
   position: absolute;
   right: 0;
  
}
.navigator-arrow2:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}
